{
  "author": "BrycensRanch & ShareX Team",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "brycengranville@outlook.com",
      "strict_min_version": "109.0"
    }
  },
  "description": "Adds 'Upload with SnapX' button to image, video, audio and text selection context menu.",
  "homepage_url": "https://github.com/BrycensRanch/SnapX",
  "icons": {
    "16": "Icons/SnapX-16.png",
    "48": "Icons/SnapX-48.png",
    "128": "Icons/SnapX-128.png",
    "256": "Icons/SnapX-128.png"
  },
  "manifest_version": 3,
  "name": "SnapX",
  "permissions": [
    "contextMenus",
    "nativeMessaging"
  ],
  "version": "0.1.0"
}
